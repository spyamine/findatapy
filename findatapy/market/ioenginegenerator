"""
IOEngineGenerator

Returns IOEngine related to the specified engine using load_engine function
"""

from findatapy.util import LoggerManager, ConfigManager

class IOEngineGenerator(object):
    def __init__(self):
        """

        :param engine: name of the engine needed
        """
        self.config = ConfigManager()
        self.logger = LoggerManager().getLogger(__name__)


    def load_engine(self,engine):

        if engine == "Arctic":
            from findatapy.market.ioenginearctic import IOEngineArctic
            return IOEngineArctic()
        if engine == "Excel":
            pass
        
        if engine == "hd5":
            pass
